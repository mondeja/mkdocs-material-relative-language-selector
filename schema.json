{
  "$schema": "https://json-schema.org/draft-07/schema",
  "title": "Relative access between translations using mkdocs-material theme.",
  "oneOf": [
    {
      "markdownDescription": "https://github.com/mondeja/mkdocs-material-relative-language-selector#readme",
      "enum": ["material-relative-language-selector"]
    },
    {
      "type": "object",
      "properties": {
        "material-relative-language-selector": {
          "markdownDescription": "https://github.com/mondeja/mkdocs-material-relative-language-selector#readme",
          "type": "object",
          "properties": {
            "root_domain": {
              "title": "If the site is served in the root of a domain you must enable this setting.",
              "markdownDescription": "https://github.com/mondeja/mkdocs-material-relative-language-selector#configuration",
              "type": "boolean",
              "default": true
            }
          },
          "additionalProperties": false
        }
      },
      "additionalProperties": false
    }
  ]
}
